# GitHub Authentication
GITHUB_TOKEN=your_github_token_here
GITHUB_CLIENT_ID=your_github_oauth_client_id_here
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret_here

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_subscription_id_here
AZURE_TENANT_ID=your_tenant_id_here
AZURE_CLIENT_ID=your_azure_client_id_here

# Container Registry
ACR_NAME=your_acr_name_here

# Azure Container Apps
ACA_RESOURCE_GROUP=your_aca_resource_group_here
ACA_JOB_NAME=your_aca_job_name_here
ACA_JOB_IMAGE=your_aca_job_image_here

# Log Analytics
LOG_ANALYTICS_WORKSPACE=your_log_analytics_workspace_here
LOG_ANALYTICS_WORKSPACE_ID=your_log_analytics_workspace_id_here

# Backend Configuration
TD_BACKEND_BASE_URL=your_backend_base_url_here
TD_BACKEND_FUNCTION_KEY=your_backend_function_key_here
# Older variable names (keeping for backward compatibility)
BACKEND_BASE_URL=
BACKEND_FUNCTION_KEY=

# Azure Functions Storage (for local development)
AzureWebJobsStorage=UseDevelopmentStorage=true
FUNCTIONS_WORKER_RUNTIME=node